{
    "nodes": [
        {
            "id": "source1",
            "type": "CsvSource",
            "path": "resources/input.csv",
            "fields": ["age", "name"],
            "types": ["usize", "String"]
        },
        {
            "id": "filter1",
            "type": "Filter",
            "condition": "row.age > 18"
        },
        {
            "id": "map1",
            "type": "Map",
            "function": "row.name.to_uppercase()"
        },
        {
            "id": "collect1",
            "type": "CollectVec"
        }
    ],
    "edges": {
        "source1": "filter1",
        "filter1": "map1",
        "map1": "collect1"
    },
    "configuration": "default",
    "async_mode": false
}